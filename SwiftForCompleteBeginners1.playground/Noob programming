let celsius =  15
var farenheit = ((celsius * 9) / 5) + 32
print("The temperature in Celsius is \(celsius)ยบ")
print("That same temperature in Farenheit is \(farenheit)ยบ")

let animals = ["Lara", "Moira", "Moira"]
print(animals.count)
let animalsUniqueCount = Set(animals)
print(animalsUniqueCount.count)
